{
    "format_version": "1.8.0",
    "render_controllers": {
        "controller.render.dead_wither": {
            "arrays": {
                "textures": {
                    "Array.wither_state": ["Texture.invulnerable", "Texture.default"]
                }
            },
            "geometry": "Geometry.default",
            "materials": [
                {
                    "*": "Material.default"
                }
            ],
            "textures": ["Array.wither_state[variable.display_normal_skin]"],
            "ignore_lighting": true
        },
        "controller.render.dead_wither_armor_white": {
            "geometry": "Geometry.armor",
            "materials": [
                {
                    "*": "Material.armor"
                }
            ],
            "textures": ["Texture.armor_white"],
            "overlay_color": {
                "r": "0",
                "g": "0",
                "b": "0",
                "a": "0"
            },
            "uv_anim": {
                "offset": ["Math.cos(query.life_time * 22.92) * 1.5", "query.life_time * 0.2"],
                "scale": [1.0, 1.0]
            },
            "light_color_multiplier": 0.8,
            "ignore_lighting": true
        },
        "controller.render.dead_wither_armor_blue": {
            "geometry": "Geometry.armor",
            "materials": [
                {
                    "*": "Material.armor"
                }
            ],
            "textures": ["Texture.armor_blue"],
            "overlay_color": {
                "r": "0",
                "g": "0",
                "b": "0",
                "a": "0"
            },
            "uv_anim": {
                "offset": ["Math.cos(query.life_time * 22.92) * 1.5", "query.life_time * 0.2"],
                "scale": [1.0, 1.0]
            },
            "light_color_multiplier": 0.8,
            "ignore_lighting": true
        }        
    }
}